   "source": [
    "from xml.dom import minidom\n",
    "\n",
    "doc = minidom.parse(\"sample(Bài 2.3).xml\")\n",
    "\n",
    "all_elements = doc.getElementsByTagName(\"*\")\n",
    "\n",
    "print(\"Danh sách tên các phần tử trong file sample.xml:\")\n",
    "for elem in all_elements:\n",
    "    print(\"-\", elem.tagName)\n"
   ]
